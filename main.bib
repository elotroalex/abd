@book{penkovaSituacionPerspectivasBibliotecas2007,
  title = {{Situaci\'on y perspectivas de las bibliotecas y centros de documentaci\'on en la Educaci\'on Superior Dominicana - E-LIS repository}},
  author = {Penkova, Snejanka},
  year = {2007},
  publisher = {{Secretar\'ia de Estado de Educaci\'on Superior Ciencia y Tecnolog\'ia}},
  address = {{Santo Domingo, Rep\'ublica Dominicana}},
  abstract = {Un diagn\'ostico basado en el estado actual de las bibliotecas universitarias y centros de informaci\'on de las universidades dominicanas. Los m\'etodos utilizados: cuantitativo (encuestas) y cualitativo (grupos focales). Este estudio se centra m\'as en la situaci\'on real que en las perspectivas de las bibliotecas universitarias dominicanas. Los principales resultados son - una participaci\'on limitada (38\% de los directores de bibliotecas) en la planificaci\'on universitaria acad\'emica y financiera, parte significativa de estas bibliotecas tienen m\'etodos adecuados de desarrollo de las colecciones, pero no necesariamente poseen colecciones actualizadas. Los horarios de servicio de la mayor\'ia de las bibliotecas apoyan las necesidades institucionales. Las bibliotecas dicen que poseen diversos medios tecnol\'ogicos. No es as\'i con la cantidad y su distribuci\'on. El hallazgo m\'as significativo es que s\'olo el 7\% de bibliotecario en los bibliotecarios acad\'emicos dominicanos tienen preparados profesionales acad\'emicos en el campo correspondiente. Adem\'as se observa un desbalance salarial muy marcado en el personal bibliotecario de una biblioteca y entre las bibliotecas.},
  file = {files/19319/penkovasituacionperspectivasbibliotecas2007.pdf},
  language = {es}
}

